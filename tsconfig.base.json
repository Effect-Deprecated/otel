{
  "compilerOptions": {
    "moduleDetection": "force",
    "composite": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "preserveSymlinks": true,
    "moduleResolution": "node",
    "noEmit": false,
    "lib": [
      "ES2021"
    ],
    "sourceMap": true,
    "declarationMap": true,
    "strict": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noErrorTruncation": false,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true,
    "stripInternal": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "target": "ES2021",
    "module": "ES6",
    "incremental": true,
    "removeComments": false,
    "paths": {
      "@effect/otel": [
        "./packages/otel/_src/index.ts"
      ],
      "@effect/otel/test/*": [
        "./packages/otel/_test/*"
      ],
      "@effect/otel/examples/*": [
        "./packages/otel/_examples/*"
      ],
      "@effect/otel/*": [
        "./packages/otel/_src/*"
      ],
      "@effect/otel-exporter-jaeger": [
        "./packages/otel-exporter-jaeger/_src/index.ts"
      ],
      "@effect/otel-exporter-jaeger/test/*": [
        "./packages/otel-exporter-jaeger/_test/*"
      ],
      "@effect/otel-exporter-jaeger/examples/*": [
        "./packages/otel-exporter-jaeger/_examples/*"
      ],
      "@effect/otel-exporter-jaeger/*": [
        "./packages/otel-exporter-jaeger/_src/*"
      ],
      "@effect/otel-exporter-trace-otlp-grpc": [
        "./packages/otel-exporter-trace-otlp-grpc/_src/index.ts"
      ],
      "@effect/otel-exporter-trace-otlp-grpc/test/*": [
        "./packages/otel-exporter-trace-otlp-grpc/_test/*"
      ],
      "@effect/otel-exporter-trace-otlp-grpc/examples/*": [
        "./packages/otel-exporter-trace-otlp-grpc/_examples/*"
      ],
      "@effect/otel-exporter-trace-otlp-grpc/*": [
        "./packages/otel-exporter-trace-otlp-grpc/_src/*"
      ],
      "@effect/otel-exporter-trace-otlp-http": [
        "./packages/otel-exporter-trace-otlp-http/_src/index.ts"
      ],
      "@effect/otel-exporter-trace-otlp-http/test/*": [
        "./packages/otel-exporter-trace-otlp-http/_test/*"
      ],
      "@effect/otel-exporter-trace-otlp-http/examples/*": [
        "./packages/otel-exporter-trace-otlp-http/_examples/*"
      ],
      "@effect/otel-exporter-trace-otlp-http/*": [
        "./packages/otel-exporter-trace-otlp-http/_src/*"
      ],
      "@effect/otel-sdk-trace-node": [
        "./packages/otel-sdk-trace-node/_src/index.ts"
      ],
      "@effect/otel-sdk-trace-node/test/*": [
        "./packages/otel-sdk-trace-node/_test/*"
      ],
      "@effect/otel-sdk-trace-node/examples/*": [
        "./packages/otel-sdk-trace-node/_examples/*"
      ],
      "@effect/otel-sdk-trace-node/*": [
        "./packages/otel-sdk-trace-node/_src/*"
      ],
    }
  },
  "include": [],
  "exclude": [
    "node_modules",
    "build",
    "lib"
  ]
}
